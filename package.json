    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.19.0",
    "react-tiny-popover": "^8.0.4",
    "recoil": "^0.7.7",
    "reflect-metadata": "^0.1.13",
    "use-immer": "^0.9.0"
  },
